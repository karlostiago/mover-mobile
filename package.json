{
  "name": "mover-mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:dev": "ENVFILE=.env.development expo start",
    "start:prod": "ENVFILE=.env.production expo start",
    "android:dev": "ENVFILE=.env.development expo start --android",
    "android:prod": "ENVFILE=.env.production expo start --android",
    "ios:dev": "ENVFILE=.env.development expo start --ios",
    "ios:prod": "ENVFILE=.env.production expo start --ios",
    "web:dev": "ENVFILE=.env.development expo start --web",
    "web:prod": "ENVFILE=.env.production expo start --web",
    "install:yarn": "yarn install",
    "install:npm": "npm install",
    "build:dev": "env-cmd -f .env.development react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "build:prod": "env-cmd -f .env.production react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.2.3",
    "@expo/metro-runtime": "~3.2.1",
    "@react-aria/ssr": "^3.9.4",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "axios": "^1.7.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "expo": "^51.0.18",
    "expo-font": "~12.0.7",
    "expo-status-bar": "~1.12.1",
    "link": "^2.1.1",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.74.3",
    "react-native-animatable": "^1.4.0",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "styled-components": "^6.1.11",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native-dotenv": "^0.2.2",
    "http-proxy-middleware": "^3.0.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-svg-transformer": "^1.4.0"
  },
  "private": true
}
